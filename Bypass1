local LocalPlayer = game:GetService("Players").LocalPlayer

if type(getconnections) ~= 'function' then -- Could have used Iris Compatibility script but it crashes for some reason
return
LocalPlayer:Kick("Missing getconnections!")
end

function onCharacterAdded()
LocalPlayer.Character.SprintScript.Disabled = true
for i,v in pairs(getconnections(LocalPlayer.Character.Humanoid.Changed)) do
v:Disable()
wait()
end
end

onCharacterAdded()
LocalPlayer.CharacterAdded:Connect(onCharacterAdded)
