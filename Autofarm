local AkaliNotif = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/Dynissimo/main/Scripts/AkaliNotif.lua"))()
local LocalPlayer = game:GetService("Players").LocalPlayer
local RunService = game:GetService("RunService")
local PlayerModule = require(LocalPlayer.PlayerScripts:WaitForChild("PlayerModule"))
local Controls = PlayerModule:GetControls()

local gameSettings = {
gameRunning = false,
GotToEnd = false,
PlayerWinsText,
RedLightGreenLightTime = "1:35",
ShapesTime = "0:00",
BloodRisingTime = "1:35",
PadCFrame = CFrame.new(-171, 13, -666),
TreeCFrame = CFrame.new(304, 6, -1111),
ShapeCFrame = CFrame.new(-846, 115, 491),
BloodRisingEnd = CFrame.new(1595, 275, 837)
}

local Variables = {
GetLobby = workspace.Lobby,
GetRLGL = workspace.Game1,
GetRLGLClock = workspace.Game1.Clock,
GetShapes = workspace.Game2,
GetShapesClock = workspace.Game2.Clock,
GetBR = workspace.BloodRising,
GetBRClock = workspace.BloodRising.Clock1
}

local Notify = AkaliNotif.Notify;
function notify(text,duration)
Notify({
Description = text;
Title = "Fish Game";
Duration = duration;
});
end

if (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude < 200 and LocalPlayer.Character.Humanoid.Health ~= 0 then --// Checking if you are at spawn and not in game
Controls:Disable()
LocalPlayer.Character:BreakJoints()
notify("Before Autofarming, Let me reset your Character just incase your data hasn't loaded in yet...",5)
elseif (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude > 200 and LocalPlayer.Character.Humanoid.Health ~= 0 then
notify("Execute when you're back at Lobby, I don't want to ruin your experience in the game...",5)
return
end

-- // It's to avoid being killed...
pcall(function()
Variables.GetShapes.Walls:Destroy()
Variables.GetShapes.DeathPit:Destroy()
Variables.GetBR.Blood:Destroy()
end)

-- // Teleporting to the Pad to begin Autofarm...
RunService.RenderStepped:Connect(function()
if (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude < 200 and LocalPlayer.Character.Humanoid.Health ~= 0 then --// Checking if you are at spawn and not in game
LocalPlayer.Character.HumanoidRootPart.CFrame = gameSettings.PadCFrame
end
end)

-- // Checking the time for Red Light, Green Light and teleporting on time...
Variables.GetRLGLClock.SurfaceGui.Frame.Counter.Changed:Connect(function(getchanged)
if getchanged == "Text" and Variables.GetRLGLClock.SurfaceGui.Frame.Counter.Text == gameSettings.RedLightGreenLightTime and Variables.GetLobby.Details2.SurfaceGui.Frame.Title.Text ~= "Blood Rising" and LocalPlayer.Character.Humanoid.Health ~= 0 and (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude > 200 then
workspace["Meshes/SM_Env_Tree_Dead_04"].CanCollide = false
LocalPlayer.Character.HumanoidRootPart.CFrame = gameSettings.TreeCFrame
gameSettings.GotToEnd = true
notify("Teleported to the end. Please <b><font color=\"rgb(245, 205, 48)\">WAIT</font></b> until the round is over to get your win.",15)
end
end)

-- // Checking the time for Shapes and teleporting on time (checks if game running here instead of collision detection for it)...
Variables.GetShapesClock.SurfaceGui.Frame.Counter.Changed:Connect(function(getchanged)
if getchanged == "Text" and Variables.GetShapesClock.SurfaceGui.Frame.Counter.Text == gameSettings.ShapesTime and Variables.GetLobby.Details2.SurfaceGui.Frame.Title.Text == "Day 2" and not gameSettings.gameRunning and LocalPlayer.Character.Humanoid.Health ~= 0 and (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude > 200 then
LocalPlayer.Character.HumanoidRootPart.CFrame = gameSettings.ShapeCFrame
gameSettings.gameRunning = true
gameSettings.GotToEnd = true
gameSettings.PlayerWinsText = LocalPlayer.PlayerGui.ScreenGui.Sub.Stats.Rank.Winst.Text
notify("Please <b><font color=\"rgb(245, 205, 48)\">WAIT</font></b> until the round is over to get your win.",15)
end
end)

-- // Checking the time for Blood Rising and teleporting on time...
Variables.GetBRClock.SurfaceGui.Frame.Counter.Changed:Connect(function(getchanged)
if getchanged == "Text" and Variables.GetBRClock.SurfaceGui.Frame.Counter.Text == gameSettings.BloodRisingTime and Variables.GetLobby.Details2.SurfaceGui.Frame.Title.Text == "Blood Rising" and LocalPlayer.Character.Humanoid.Health ~= 0  and (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude > 200 then
LocalPlayer.Character.HumanoidRootPart.CFrame = gameSettings.BloodRisingEnd
gameSettings.GotToEnd = true
notify("Teleported to the end. Please <b><font color=\"rgb(245, 205, 48)\">WAIT</font></b> until the round is over to get your win.",15)
end
end)

-- // RedLightGreenLight Wall CanCollide Detection...
Variables.GetRLGL.StartWall.Changed:Connect(function(getchanged)
if getchanged == "CanCollide" and Variables.GetRLGL.StartWall.CanCollide == false and Variables.GetLobby.Details2.SurfaceGui.Frame.Title.Text ~= "Blood Rising" and (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude > 200 then
gameSettings.gameRunning = true
gameSettings.PlayerWinsText = LocalPlayer.PlayerGui.ScreenGui.Sub.Stats.Rank.Winst.Text
notify("Round Started, Teleporting at <b><font color=\"rgb(245, 205, 48)\">1:35</font></b>...",40)
end
end)

-- // BloodRising Wall CanCollide Detection...
Variables.GetBR.StartWall2.Changed:Connect(function(getchanged)
if getchanged == "CanCollide" and Variables.GetBR.StartWall2.CanCollide == false and Variables.GetLobby.Details2.SurfaceGui.Frame.Title.Text == "Blood Rising" and (LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - workspace.Lobby.Respawn.Position).magnitude > 200 then
gameSettings.gameRunning = true
gameSettings.PlayerWinsText = LocalPlayer.PlayerGui.ScreenGui.Sub.Stats.Rank.Winst.Text
notify("Round Started, Teleporting at <b><font color=\"rgb(245, 205, 48)\">1:35</font></b>...",40)
end
end)

-- // This is how we determine when the round is over and our win was given...
LocalPlayer.PlayerGui.ScreenGui.Sub.Stats.Rank.Winst.Changed:Connect(function(getchanged)
if getchanged == "Text" and gameSettings.PlayerWinsText ~= LocalPlayer.PlayerGui.ScreenGui.Sub.Stats.Rank.Winst.Text and gameSettings.gameRunning and gameSettings.GotToEnd then
notify("Round over, You have <b><font color=\"rgb(245, 205, 48)\">gained</font></b> a win...\nOld Score: "..gameSettings.PlayerWinsText.." | New Score: "..LocalPlayer.PlayerGui.ScreenGui.Sub.Stats.Rank.Winst.Text,20)
gameSettings.gameRunning = false
gameSettings.GotToEnd = false
end
end)
